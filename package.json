{
  "name": "my-changelog-test",
  "version": "1.4.7",
  "description": "",
  "private": true,
  "main": "index.js",
  "scripts": {
    "copy": "rimraf dist",
    "build": "npm run copy && tsc && node build/index.js",
    "dev": "set NODE_ENV=dev&& nodemon ./index",
    "lint": "eslint src --fix --ext .js,.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "commit": "git add . && git cz",
    "commit2": "git add . && gitmoji -c",
    "changelog": "conventional-changelog -p cmyr-config -i CHANGELOG.md -s -r 0",
    "rm": "rimraf node_modules",
    "watch": "npm run copy && tsc -w",
    "ver": "standard-version",
    "release": "cross-env DEBUG=conventional-changelog:* semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaoMeiYouRen/my-changelog-test.git"
  },
  "author": "CaoMeiYouRen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CaoMeiYouRen/my-changelog-test/issues"
  },
  "homepage": "https://github.com/CaoMeiYouRen/my-changelog-test#readme",
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "download": "^8.0.0",
    "fs-extra": "^9.0.1",
    "got": "^9.6.0",
    "md5": "2.3.0",
    "minimist": "^1.2.5",
    "request": "^2.88.0",
    "tunnel": "^0.0.6"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/download": "^6.2.4",
    "@types/fs-extra": "^9.0.6",
    "@types/md5": "^2.2.0",
    "@types/node": "^14.14.20",
    "@types/request": "^2.48.3",
    "@types/tunnel": "^0.0.1",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.13.0",
    "commitizen": "^4.0.4",
    "commitlint": "^11.0.0",
    "compare-func": "^2.0.0",
    "compressing": "^1.5.1",
    "conventional-changelog-cli": "^2.0.28",
    "conventional-changelog-cmyr-config": "^1.2.0",
    "conventional-changelog-custom-config": "^0.3.1",
    "conventional-github-releaser": "^3.1.3",
    "conventional-recommended-bump": "^6.1.0",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.0.2",
    "dateformat": "^4.4.1",
    "eslint": "^7.17.0",
    "eslint-config-cmyr": "1.0.10",
    "gitmoji-cli": "^3.2.18",
    "gulp": "^4.0.2",
    "husky": "^4.3.7",
    "install": "^0.13.0",
    "lint-staged": "^10.2.0",
    "release-it": "^14.2.1",
    "rimraf": "^3.0.0",
    "semantic-release": "^17.3.1",
    "semantic-release-gitmoji": "^1.3.3",
    "standard-version": "^9.1.0",
    "typescript": "^4.1.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    ".{ts,js,vue}": [
      "npm run lint",
      "git add ."
    ]
  },
  "changelog": {
    "emojis": true,
    "authorName": false,
    "authorEmail": false,
    "settings": {
      "feat": {
        "title": "✨ 新功能"
      },
      "fix": {
        "title": "🐛 Bug 修复"
      },
      "perf": {
        "title": "⚡ 性能优化"
      },
      "revert": {
        "title": "⏪ 回退"
      },
      "refactor": {
        "title": "♻ 代码重构"
      },
      "docs": {
        "title": "📝 文档",
        "enable": false
      },
      "style": {
        "title": "💄 风格",
        "enable": false
      },
      "test": {
        "title": "✅ 测试",
        "enable": false
      },
      "build": {
        "title": "👷‍ 构建",
        "enable": false
      },
      "ci": {
        "title": "🔧 CI 配置",
        "enable": false
      },
      "chore": {
        "title": "🎫 其他更新",
        "enable": false
      }
    }
  }
}
